{
    "name": "hck/img-captcha",
    "description": "captcha package for thinkphp6",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hck",
            "email": "hongchenker@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.3",
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "imgcaptcha\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "config":{
                "captcha": "src/config.php"
            }
        }
    }
}